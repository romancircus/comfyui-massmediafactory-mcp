{
  "_meta": {
    "type": "skeleton",
    "model": "LTX-Video",
    "workflow": "image-to-video",
    "sampler_type": "SamplerCustom",
    "notes": "Adds LTXVPreprocess and LTXVImgToVideo to T2V skeleton."
  },
  "nodes": [
    {"id": "1", "type": "CheckpointLoaderSimple", "outputs": ["MODEL", "CLIP", "VAE"]},
    {"id": "2", "type": "CLIPLoader", "outputs": ["CLIP"]},
    {"id": "3", "type": "CLIPTextEncode", "role": "positive", "outputs": ["CONDITIONING"]},
    {"id": "4", "type": "CLIPTextEncode", "role": "negative", "outputs": ["CONDITIONING"]},
    {"id": "5", "type": "LTXVConditioning", "outputs": ["CONDITIONING+", "CONDITIONING-"]},
    {"id": "6", "type": "LoadImage", "outputs": ["IMAGE", "MASK"]},
    {"id": "7", "type": "LTXVPreprocess", "outputs": ["IMAGE"]},
    {"id": "8", "type": "LTXVImgToVideo", "outputs": ["LATENT"]},
    {"id": "9", "type": "LTXVScheduler", "outputs": ["SIGMAS"]},
    {"id": "10", "type": "KSamplerSelect", "outputs": ["SAMPLER"]},
    {"id": "11", "type": "SamplerCustom", "outputs": ["LATENT", "LATENT_DENOISED"]},
    {"id": "12", "type": "VAEDecode", "outputs": ["IMAGE"]},
    {"id": "13", "type": "SaveAnimatedWEBP", "outputs": []}
  ],
  "connections": [
    {"from": "1.MODEL", "to": "11.model"},
    {"from": "1.VAE", "to": "8.vae"},
    {"from": "1.VAE", "to": "12.vae"},
    {"from": "2.CLIP", "to": "3.clip"},
    {"from": "2.CLIP", "to": "4.clip"},
    {"from": "3.CONDITIONING", "to": "5.positive"},
    {"from": "4.CONDITIONING", "to": "5.negative"},
    {"from": "5.CONDITIONING+", "to": "11.positive"},
    {"from": "5.CONDITIONING-", "to": "11.negative"},
    {"from": "6.IMAGE", "to": "7.image"},
    {"from": "7.IMAGE", "to": "8.image"},
    {"from": "8.LATENT", "to": "11.latent_image"},
    {"from": "9.SIGMAS", "to": "11.sigmas"},
    {"from": "10.SAMPLER", "to": "11.sampler"},
    {"from": "11.LATENT", "to": "12.samples"},
    {"from": "12.IMAGE", "to": "13.images"}
  ],
  "defaults": {
    "7.strength": 40,
    "8.width": 768,
    "8.height": 512,
    "8.length": 97,
    "9.steps": 30,
    "9.max_shift": 2.05,
    "9.base_shift": 0.95,
    "10.sampler_name": "euler",
    "11.cfg": 3.0,
    "11.denoise": 0.9
  }
}
