{
  "_meta": {
    "description": "ControlNet Canny edge-guided generation for structure control",
    "model": "FLUX.2 or SDXL",
    "type": "txt2img_controlnet",
    "parameters": ["PROMPT", "IMAGE_PATH", "CANNY_LOW", "CANNY_HIGH", "CONTROL_STRENGTH", "SEED", "WIDTH", "HEIGHT"],
    "defaults": {
      "WIDTH": 1024,
      "HEIGHT": 1024,
      "SEED": 42,
      "CANNY_LOW": 100,
      "CANNY_HIGH": 200,
      "CONTROL_STRENGTH": 0.8,
      "PROMPT": "A scene guided by edge structure"
    },
    "required_models": ["ControlNet-Canny"]
  },
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "flux2-dev-fp8.safetensors"
    }
  },
  "2": {
    "class_type": "ControlNetLoader",
    "inputs": {
      "control_net_name": "control_v11p_sd15_canny.pth"
    }
  },
  "3": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{IMAGE_PATH}}"
    }
  },
  "4": {
    "class_type": "CannyEdgePreprocessor",
    "inputs": {
      "image": ["3", 0],
      "low_threshold": "{{CANNY_LOW}}",
      "high_threshold": "{{CANNY_HIGH}}"
    }
  },
  "5": {
    "class_type": "ControlNetApply",
    "inputs": {
      "control_net": ["2", 0],
      "image": ["4", 0],
      "strength": "{{CONTROL_STRENGTH}}"
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["1", 1],
      "text": "{{PROMPT}}"
    }
  },
  "7": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "batch_size": 1
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["1", 0],
      "positive": ["6", 0],
      "negative": ["6", 0],
      "latent_image": ["7", 0],
      "seed": "{{SEED}}",
      "steps": 25,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "control_net": ["5", 0]
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["1", 2]
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["9", 0],
      "filename_prefix": "controlnet_canny"
    }
  }
}
