{
  "_meta": {
    "description": "IP-Adapter style transfer using reference image",
    "model": "FLUX.2 or SDXL",
    "type": "txt2img_ipadapter",
    "parameters": ["PROMPT", "STYLE_IMAGE_PATH", "IP_WEIGHT", "SEED", "WIDTH", "HEIGHT"],
    "defaults": {
      "WIDTH": 1024,
      "HEIGHT": 1024,
      "SEED": 42,
      "IP_WEIGHT": 0.8,
      "PROMPT": "A scene in the reference style"
    },
    "required_models": ["IP-Adapter"]
  },
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "flux2-dev-fp8.safetensors"
    }
  },
  "2": {
    "class_type": "IPAdapterModelLoader",
    "inputs": {
      "model_name": "ip-adapter_sd15.safetensors"
    }
  },
  "3": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{STYLE_IMAGE_PATH}}"
    }
  },
  "4": {
    "class_type": "IPAdapterEncoder",
    "inputs": {
      "ipadapter": ["2", 0],
      "image": ["3", 0]
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["1", 1],
      "text": "{{PROMPT}}"
    }
  },
  "6": {
    "class_type": "IPAdapterApply",
    "inputs": {
      "ipadapter": ["2", 0],
      "embeds": ["4", 0],
      "weight": "{{IP_WEIGHT}}",
      "conditioning": ["5", 0]
    }
  },
  "7": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "batch_size": 1
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["1", 0],
      "positive": ["6", 0],
      "negative": ["5", 0],
      "latent_image": ["7", 0],
      "seed": "{{SEED}}",
      "steps": 25,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["1", 2]
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["9", 0],
      "filename_prefix": "ipadapter_style"
    }
  }
}
