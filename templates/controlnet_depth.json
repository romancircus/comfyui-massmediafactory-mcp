{
  "_meta": {
    "description": "ControlNet Depth-guided generation for composition control",
    "model": "FLUX.2 or SDXL",
    "type": "txt2img_controlnet",
    "parameters": ["PROMPT", "DEPTH_MAP_PATH", "CONTROL_STRENGTH", "SEED", "WIDTH", "HEIGHT"],
    "defaults": {
      "WIDTH": 1024,
      "HEIGHT": 1024,
      "SEED": 42,
      "CONTROL_STRENGTH": 0.8,
      "PROMPT": "A scene guided by depth map"
    },
    "required_models": ["ControlNet-Depth"]
  },
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "flux2-dev-fp8.safetensors"
    }
  },
  "2": {
    "class_type": "ControlNetLoader",
    "inputs": {
      "control_net_name": "control_v11f1p_sd15_depth.pth"
    }
  },
  "3": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{DEPTH_MAP_PATH}}"
    }
  },
  "4": {
    "class_type": "ControlNetApply",
    "inputs": {
      "control_net": ["2", 0],
      "image": ["3", 0],
      "strength": "{{CONTROL_STRENGTH}}"
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["1", 1],
      "text": "{{PROMPT}}"
    }
  },
  "6": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "batch_size": 1
    }
  },
  "7": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["1", 0],
      "positive": ["5", 0],
      "negative": ["5", 0],
      "latent_image": ["6", 0],
      "seed": "{{SEED}}",
      "steps": 25,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "control_net": ["4", 0]
    }
  },
  "8": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["7", 0],
      "vae": ["1", 2]
    }
  },
  "9": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["8", 0],
      "filename_prefix": "controlnet_depth"
    }
  }
}
