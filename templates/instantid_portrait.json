{
  "_meta": {
    "description": "InstantID face-guided generation with identity preservation",
    "model": "FLUX.2 or SDXL",
    "type": "txt2img_instantid",
    "parameters": ["PROMPT", "FACE_IMAGE_PATH", "IDENTITY_STRENGTH", "SEED", "WIDTH", "HEIGHT"],
    "defaults": {
      "WIDTH": 1024,
      "HEIGHT": 1024,
      "SEED": 42,
      "IDENTITY_STRENGTH": 0.8,
      "PROMPT": "Portrait photo of the person"
    },
    "required_models": ["InstantID", "InsightFace"]
  },
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "flux2-dev-fp8.safetensors"
    }
  },
  "2": {
    "class_type": "InstantIDModelLoader",
    "inputs": {
      "model_name": "ip-adapter_instant_id_sdxl.bin"
    }
  },
  "3": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{FACE_IMAGE_PATH}}"
    }
  },
  "4": {
    "class_type": "InstantIDFaceAnalysis",
    "inputs": {
      "image": ["3", 0]
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": ["1", 1],
      "text": "{{PROMPT}}"
    }
  },
  "6": {
    "class_type": "InstantIDApply",
    "inputs": {
      "instantid": ["2", 0],
      "face_embeds": ["4", 0],
      "strength": "{{IDENTITY_STRENGTH}}",
      "conditioning": ["5", 0]
    }
  },
  "7": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "batch_size": 1
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["1", 0],
      "positive": ["6", 0],
      "negative": ["5", 0],
      "latent_image": ["7", 0],
      "seed": "{{SEED}}",
      "steps": 30,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["1", 2]
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["9", 0],
      "filename_prefix": "instantid_portrait"
    }
  }
}
