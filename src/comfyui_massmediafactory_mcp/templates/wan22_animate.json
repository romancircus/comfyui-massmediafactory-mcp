{
  "_meta": {
    "description": "Wan 2.2 Animate-14B character animation with pose/motion control",
    "model": "Wan 2.2 Animate 14B",
    "type": "animate",
    "parameters": [
      "CHARACTER_IMAGE",
      "PROMPT",
      "NEGATIVE",
      "SEED",
      "WIDTH",
      "HEIGHT",
      "FRAMES",
      "FPS",
      "STEPS",
      "CFG",
      "SHIFT",
      "POSE_STRENGTH",
      "FACE_STRENGTH",
      "LOAD_DEVICE",
      "ATTENTION_MODE",
      "BLOCK_SWAP"
    ],
    "defaults": {
      "SEED": 42,
      "WIDTH": 832,
      "HEIGHT": 480,
      "FRAMES": 81,
      "FPS": 16,
      "STEPS": 30,
      "CFG": 5.0,
      "SHIFT": 5.0,
      "POSE_STRENGTH": 1.0,
      "FACE_STRENGTH": 1.0,
      "LOAD_DEVICE": "offload_device",
      "ATTENTION_MODE": "sdpa",
      "BLOCK_SWAP": 20,
      "PROMPT": "smooth natural motion, high quality character animation",
      "NEGATIVE": "worst quality, blurry, jittery, distorted face, identity loss"
    },
    "vram_min": 24,
    "tags": [
      "model:wan",
      "type:animate",
      "priority:recommended"
    ],
    "version": "1.0.0",
    "notes": "Character animation: preserves identity from ref image, uses WanVideoAnimateEmbeds with face/pose strength control. No motion video required â€” works with just a character reference image + text prompt. For motion transfer, provide pose_images via custom workflow."
  },
  "1": {
    "class_type": "WanVideoBlockSwap",
    "inputs": {
      "blocks_to_swap": "{{BLOCK_SWAP}}",
      "offload_img_emb": true,
      "offload_txt_emb": true
    }
  },
  "2": {
    "class_type": "WanVideoModelLoader",
    "inputs": {
      "model": "wan_animate/Wan2_2-Animate-14B_fp8_e4m3fn_scaled_KJ.safetensors",
      "base_precision": "bf16",
      "quantization": "disabled",
      "load_device": "{{LOAD_DEVICE}}",
      "attention_mode": "{{ATTENTION_MODE}}",
      "block_swap_args": [
        "1",
        0
      ]
    }
  },
  "3": {
    "class_type": "WanVideoVAELoader",
    "inputs": {
      "model_name": "wan_2.1_vae.safetensors",
      "precision": "bf16"
    }
  },
  "4": {
    "class_type": "CLIPVisionLoader",
    "inputs": {
      "clip_name": "clip_vision_h.safetensors"
    }
  },
  "5": {
    "class_type": "LoadImage",
    "_meta": { "title": "Character Reference Image" },
    "inputs": {
      "image": "{{CHARACTER_IMAGE}}"
    }
  },
  "6": {
    "class_type": "LoadWanVideoT5TextEncoder",
    "inputs": {
      "model_name": "umt5_xxl_fp16.safetensors",
      "precision": "bf16",
      "load_device": "offload_device"
    }
  },
  "7": {
    "class_type": "WanVideoTextEncode",
    "inputs": {
      "positive_prompt": "{{PROMPT}}",
      "negative_prompt": "{{NEGATIVE}}",
      "t5": [
        "6",
        0
      ],
      "force_offload": true
    }
  },
  "8": {
    "class_type": "WanVideoClipVisionEncode",
    "inputs": {
      "clip_vision": [
        "4",
        0
      ],
      "image_1": [
        "5",
        0
      ],
      "strength_1": 1.0,
      "strength_2": 1.0,
      "crop": "center",
      "combine_embeds": "average",
      "force_offload": true
    }
  },
  "9": {
    "class_type": "WanVideoAnimateEmbeds",
    "_meta": { "title": "Animate Embeds (Identity + Pose Control)" },
    "inputs": {
      "vae": [
        "3",
        0
      ],
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "num_frames": "{{FRAMES}}",
      "force_offload": true,
      "frame_window_size": 77,
      "colormatch": "disabled",
      "pose_strength": "{{POSE_STRENGTH}}",
      "face_strength": "{{FACE_STRENGTH}}",
      "clip_embeds": [
        "8",
        0
      ],
      "ref_images": [
        "5",
        0
      ]
    }
  },
  "10": {
    "class_type": "WanVideoSampler",
    "inputs": {
      "model": [
        "2",
        0
      ],
      "image_embeds": [
        "9",
        0
      ],
      "text_embeds": [
        "7",
        0
      ],
      "steps": "{{STEPS}}",
      "cfg": "{{CFG}}",
      "shift": "{{SHIFT}}",
      "seed": "{{SEED}}",
      "force_offload": true,
      "scheduler": "unipc",
      "riflex_freq_index": 0
    }
  },
  "11": {
    "class_type": "WanVideoDecode",
    "inputs": {
      "vae": [
        "3",
        0
      ],
      "samples": [
        "10",
        0
      ],
      "enable_vae_tiling": false,
      "tile_x": 272,
      "tile_y": 272,
      "tile_stride_x": 144,
      "tile_stride_y": 128
    }
  },
  "12": {
    "class_type": "CreateVideo",
    "inputs": {
      "images": [
        "11",
        0
      ],
      "fps": "{{FPS}}"
    }
  },
  "13": {
    "class_type": "SaveVideo",
    "inputs": {
      "video": [
        "12",
        0
      ],
      "filename_prefix": "wan22_animate",
      "format": "mp4",
      "codec": "h264"
    }
  }
}
