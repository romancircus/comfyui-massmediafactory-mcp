{
  "_meta": {
    "description": "Wan 2.2 Subject-to-Video (S2V) workflow using WanVideoWrapper nodes",
    "model": "Wan 2.2 S2V 14B",
    "type": "img2vid",
    "parameters": [
      "IMAGE_PATH",
      "PROMPT",
      "NEGATIVE",
      "SEED",
      "FRAMES",
      "FPS",
      "STEPS",
      "CFG",
      "SHIFT",
      "WIDTH",
      "HEIGHT",
      "NOISE_AUG",
      "LOAD_DEVICE",
      "QUANTIZATION",
      "ATTENTION_MODE",
      "FORCE_OFFLOAD_SAMPLER"
    ],
    "defaults": {
      "SEED": 42,
      "FRAMES": 77,
      "FPS": 16,
      "STEPS": 30,
      "CFG": 5.0,
      "SHIFT": 5.0,
      "WIDTH": 832,
      "HEIGHT": 480,
      "NOISE_AUG": 0.0,
      "PROMPT": "smooth motion, high quality video",
      "NEGATIVE": "worst quality, blurry, jittery, static, no motion",
      "LOAD_DEVICE": "offload_device",
      "QUANTIZATION": "fp8_e4m3fn_scaled",
      "ATTENTION_MODE": "sdpa",
      "FORCE_OFFLOAD_SAMPLER": true
    },
    "notes": "Wan 2.2 S2V for subject-based animation. Uses wan2.2_s2v_14B_fp8_scaled.safetensors model. Similar to I2V but optimized for subject consistency.",
    "vram_min": 16,
    "tags": [
      "model:wan",
      "priority:recommended"
    ],
    "version": "1.0.0",
    "hub_hash": "d038ab9afcdb1e3d"
  },
  "1": {
    "class_type": "WanVideoModelLoader",
    "inputs": {
      "model": "wan_s2v_fp8/wan2.2_s2v_14B_fp8_scaled.safetensors",
      "base_precision": "bf16",
      "quantization": "{{QUANTIZATION}}",
      "load_device": "{{LOAD_DEVICE}}",
      "attention_mode": "{{ATTENTION_MODE}}"
    }
  },
  "2": {
    "class_type": "WanVideoVAELoader",
    "inputs": {
      "model_name": "wan_2.1_vae.safetensors",
      "precision": "bf16"
    }
  },
  "3": {
    "class_type": "LoadWanVideoT5TextEncoder",
    "inputs": {
      "model_name": "umt5_xxl_fp16.safetensors",
      "precision": "bf16",
      "load_device": "offload_device"
    }
  },
  "4": {
    "class_type": "LoadWanVideoClipTextEncoder",
    "inputs": {
      "model_name": "models_clip_open-clip-xlm-roberta-large-vit-huge-14.pth",
      "precision": "fp16",
      "load_device": "offload_device"
    }
  },
  "5": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{IMAGE_PATH}}"
    }
  },
  "6": {
    "class_type": "WanVideoTextEncode",
    "inputs": {
      "positive_prompt": "{{PROMPT}}",
      "negative_prompt": "{{NEGATIVE}}",
      "t5": [
        "3",
        0
      ],
      "force_offload": true
    }
  },
  "7": {
    "class_type": "WanVideoClipVisionEncode",
    "inputs": {
      "clip_vision": [
        "4",
        0
      ],
      "image_1": [
        "5",
        0
      ],
      "strength_1": 1.0,
      "strength_2": 1.0,
      "crop": "center",
      "combine_embeds": "average",
      "force_offload": true
    }
  },
  "8": {
    "class_type": "WanVideoImageToVideoEncode",
    "inputs": {
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "num_frames": "{{FRAMES}}",
      "noise_aug_strength": "{{NOISE_AUG}}",
      "start_latent_strength": 1.0,
      "end_latent_strength": 1.0,
      "force_offload": true,
      "vae": [
        "2",
        0
      ],
      "clip_embeds": [
        "7",
        0
      ],
      "start_image": [
        "5",
        0
      ]
    }
  },
  "9": {
    "class_type": "WanVideoSampler",
    "inputs": {
      "model": [
        "1",
        0
      ],
      "image_embeds": [
        "8",
        0
      ],
      "text_embeds": [
        "6",
        0
      ],
      "steps": "{{STEPS}}",
      "cfg": "{{CFG}}",
      "shift": "{{SHIFT}}",
      "seed": "{{SEED}}",
      "force_offload": "{{FORCE_OFFLOAD_SAMPLER}}",
      "scheduler": "unipc",
      "riflex_freq_index": 0
    }
  },
  "10": {
    "class_type": "WanVideoDecode",
    "inputs": {
      "vae": [
        "2",
        0
      ],
      "samples": [
        "9",
        0
      ],
      "enable_vae_tiling": false,
      "tile_x": 272,
      "tile_y": 272,
      "tile_stride_x": 144,
      "tile_stride_y": 128
    }
  },
  "11": {
    "class_type": "CreateVideo",
    "inputs": {
      "images": [
        "10",
        0
      ],
      "fps": "{{FPS}}"
    }
  },
  "12": {
    "class_type": "SaveVideo",
    "inputs": {
      "video": [
        "11",
        0
      ],
      "filename_prefix": "wan22_s2v",
      "format": "mp4",
      "codec": "h264"
    }
  }
}
