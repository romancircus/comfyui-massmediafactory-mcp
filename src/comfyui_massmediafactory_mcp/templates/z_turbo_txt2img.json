{
  "_meta": {
    "description": "Z-Image-Turbo fast text-to-image generation (4-step inference)",
    "model": "Z-Image-Turbo",
    "type": "txt2img",
    "parameters": [
      "PROMPT",
      "NEGATIVE",
      "SEED",
      "WIDTH",
      "HEIGHT",
      "STEPS",
      "CFG"
    ],
    "defaults": {
      "WIDTH": 1024,
      "HEIGHT": 1024,
      "SEED": 42,
      "STEPS": 4,
      "CFG": 1.0,
      "NEGATIVE": "blurry, low quality, distorted, text, watermark"
    },
    "notes": "Ultra-fast 4-step generation. 5x faster than standard models. CFG 1.0-2.0 for best results. Higher CFG can cause artifacts.",
    "vram_min": 16,
    "tags": [
      "model:z_turbo",
      "priority:fast"
    ],
    "version": "1.0.0",
    "hub_hash": "bab0f13b299fed56"
