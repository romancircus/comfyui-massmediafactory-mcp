  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "z-image-turbo.safetensors"
    }
  },
  "2": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": [
        "1",
        1
      ],
      "text": "{{PROMPT}}"
    }
  },
  "3": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "clip": [
        "1",
        1
      ],
      "text": "{{NEGATIVE}}"
    }
  },
  "4": {
    "class_type": "EmptyLatentImage",
    "inputs": {
