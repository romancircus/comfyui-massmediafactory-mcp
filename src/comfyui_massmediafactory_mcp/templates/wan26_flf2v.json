{
  "_meta": {
    "description": "Wan 2.1 First-Last Frame to Video. Provide start and end frames, generates smooth video transition between them.",
    "model": "Wan 2.1 I2V 14B",
    "type": "flf2vid",
    "parameters": ["START_IMAGE", "END_IMAGE", "PROMPT", "NEGATIVE", "SEED", "WIDTH", "HEIGHT", "FRAMES", "FPS", "STEPS", "CFG", "SHIFT"],
    "defaults": {
      "SEED": 42,
      "FRAMES": 81,
      "FPS": 16,
      "STEPS": 30,
      "CFG": 5.0,
      "SHIFT": 5.0,
      "WIDTH": 832,
      "HEIGHT": 480,
      "PROMPT": "smooth motion transition, high quality video",
      "NEGATIVE": "worst quality, blurry, jittery, static, abrupt changes"
    },
    "vram_min": 16,
    "tags": ["model:wan", "type:flf2v"],
    "version": "1.0.0",
    "notes": "Uses native ComfyUI WanFirstLastFrameToVideo node with UNETLoader pipeline. Provide two keyframes (start + end) and the model generates smooth video between them. Uses CLIP vision for image conditioning."
  },
  "1": {
    "class_type": "UNETLoader",
    "_meta": { "title": "Load WAN I2V UNET" },
    "inputs": {
      "unet_name": "wan_i2v_fp8/Wan2_1-I2V-14B-480p_fp8_e4m3fn_scaled_KJ.safetensors",
      "weight_dtype": "fp8_e4m3fn"
    }
  },
  "2": {
    "class_type": "CLIPLoader",
    "_meta": { "title": "Load T5 Text Encoder" },
    "inputs": {
      "clip_name": "umt5_xxl_fp16.safetensors",
      "type": "wan"
    }
  },
  "3": {
    "class_type": "VAELoader",
    "_meta": { "title": "Load WAN VAE" },
    "inputs": {
      "vae_name": "Wan2.1_VAE.safetensors"
    }
  },
  "4": {
    "class_type": "CLIPVisionLoader",
    "_meta": { "title": "Load CLIP Vision" },
    "inputs": {
      "clip_name": "models_clip_open-clip-xlm-roberta-large-vit-huge-14.pth"
    }
  },
  "5": {
    "class_type": "LoadImage",
    "_meta": { "title": "Load Start Frame" },
    "inputs": {
      "image": "{{START_IMAGE}}"
    }
  },
  "6": {
    "class_type": "LoadImage",
    "_meta": { "title": "Load End Frame" },
    "inputs": {
      "image": "{{END_IMAGE}}"
    }
  },
  "7": {
    "class_type": "CLIPTextEncode",
    "_meta": { "title": "Encode Positive Prompt" },
    "inputs": {
      "text": "{{PROMPT}}",
      "clip": ["2", 0]
    }
  },
  "8": {
    "class_type": "CLIPTextEncode",
    "_meta": { "title": "Encode Negative Prompt" },
    "inputs": {
      "text": "{{NEGATIVE}}",
      "clip": ["2", 0]
    }
  },
  "9": {
    "class_type": "CLIPVisionEncode",
    "_meta": { "title": "Encode Start Frame CLIP Vision" },
    "inputs": {
      "clip_vision": ["4", 0],
      "image": ["5", 0],
      "crop": "center"
    }
  },
  "10": {
    "class_type": "CLIPVisionEncode",
    "_meta": { "title": "Encode End Frame CLIP Vision" },
    "inputs": {
      "clip_vision": ["4", 0],
      "image": ["6", 0],
      "crop": "center"
    }
  },
  "11": {
    "class_type": "WanFirstLastFrameToVideo",
    "_meta": { "title": "First-Last Frame Conditioning" },
    "inputs": {
      "positive": ["7", 0],
      "negative": ["8", 0],
      "vae": ["3", 0],
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "length": "{{FRAMES}}",
      "batch_size": 1,
      "start_image": ["5", 0],
      "end_image": ["6", 0],
      "clip_vision_start_image": ["9", 0],
      "clip_vision_end_image": ["10", 0]
    }
  },
  "12": {
    "class_type": "ModelSamplingSD3",
    "_meta": { "title": "Apply Shift" },
    "inputs": {
      "model": ["1", 0],
      "shift": "{{SHIFT}}"
    }
  },
  "13": {
    "class_type": "KSampler",
    "_meta": { "title": "Sample Video" },
    "inputs": {
      "model": ["12", 0],
      "positive": ["11", 0],
      "negative": ["11", 1],
      "latent_image": ["11", 2],
      "seed": "{{SEED}}",
      "steps": "{{STEPS}}",
      "cfg": "{{CFG}}",
      "sampler_name": "uni_pc",
      "scheduler": "simple",
      "denoise": 1.0
    }
  },
  "14": {
    "class_type": "VAEDecode",
    "_meta": { "title": "Decode Video Latent" },
    "inputs": {
      "samples": ["13", 0],
      "vae": ["3", 0]
    }
  },
  "15": {
    "class_type": "CreateVideo",
    "_meta": { "title": "Create Video" },
    "inputs": {
      "images": ["14", 0],
      "fps": "{{FPS}}"
    }
  },
  "16": {
    "class_type": "SaveVideo",
    "_meta": { "title": "Save Video" },
    "inputs": {
      "video": ["15", 0],
      "filename_prefix": "wan_flf2v",
      "format": "mp4",
      "codec": "h264"
    }
  }
}
