{
  "_meta": {
    "description": "TeleStyle video style transfer with temporal consistency",
    "model": "TeleStyle",
    "type": "video_style_transfer",
    "parameters": [
      "VIDEO_PATH",
      "STYLE_PATH",
      "STEPS",
      "CFG",
      "SEED",
      "FPS"
    ],
    "defaults": {
      "STEPS": 12,
      "CFG": 1.0,
      "SEED": 42,
      "FPS": 24
    },
    "vram_min": 16,
    "tags": [
      "capability:style_transfer"
    ],
    "version": "1.0.0",
    "hub_hash": "395630689c976325"
  },
  "1": {
    "class_type": "TeleStyleLoader",
    "inputs": {
      "dtype": "bf16"
    }
  },
  "2": {
    "class_type": "VHS_LoadVideo",
    "inputs": {
      "video": "{{VIDEO_PATH}}",
      "force_rate": 0,
      "custom_width": 0,
      "custom_height": 0,
      "frame_load_cap": 0,
      "skip_first_frames": 0,
      "select_every_nth": 1
    }
  },
  "3": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{STYLE_PATH}}"
    }
  },
  "4": {
    "class_type": "VHS_VideoInfo",
    "inputs": {
      "video_info": [
        "2",
        3
      ]
    }
  },
  "5": {
    "class_type": "TeleStyleVideoInference",
    "inputs": {
      "model": [
        "1",
        0
      ],
      "video_frames": [
        "2",
        0
      ],
      "style_image": [
        "3",
        0
      ],
      "steps": "{{STEPS}}",
      "cfg": "{{CFG}}",
      "seed": "{{SEED}}",
      "scheduler": "DPM++",
      "fast_mode": true,
      "enable_tiling": false,
      "acceleration": "default"
    }
  },
  "6": {
    "class_type": "VHS_VideoCombine",
    "inputs": {
      "images": [
        "5",
        0
      ],
      "audio": [
        "2",
        2
      ],
      "frame_rate": "{{FPS}}",
      "loop_count": 0,
      "filename_prefix": "telestyle_output",
      "format": "video/h264-mp4",
      "pingpong": false,
      "save_output": true
    }
  }
}
