{
  "_meta": {
    "description": "F5-TTS Text-to-Speech generation for high-quality voice synthesis",
    "type": "audio_tts",
    "use_case": "Generate spoken audio from text (replaces ElevenLabs)",
    "replaces": "ElevenLabs TTS",
    "parameters": [
      "TEXT",
      "REFERENCE_AUDIO",
      "REFERENCE_TEXT",
      "SPEED",
      "SEED"
    ],
    "defaults": {
      "SPEED": 1.0,
      "SEED": 42
    },
    "agent_notes": {
      "TEXT": "The text to be spoken (max ~500 chars per generation)",
      "REFERENCE_AUDIO": "Optional: Path to reference voice audio (3-10 seconds ideal)",
      "REFERENCE_TEXT": "Optional: Transcript of reference audio for voice cloning",
      "SPEED": "Speech speed multiplier (0.5-2.0)",
      "output_formula": "For video sync: video_frames = (audio_duration_seconds * fps)"
    },
    "custom_nodes_required": [
      "ComfyUI-F5-TTS"
    ],
    "installation": "Install via ComfyUI Manager: search 'F5-TTS'",
    "vram_min": 4,
    "tags": [
      "type:audio",
      "capability:tts",
      "model:f5_tts"
    ],
    "model": "audio"
  },
  "1": {
    "class_type": "F5TTSModelLoader",
    "_comment": "Load F5-TTS model (downloads automatically on first use)",
    "inputs": {
      "model": "F5-TTS",
      "vocoder": "vocos"
    }
  },
  "2": {
    "class_type": "F5TTSGenerate",
    "_comment": "Generate speech from text",
    "inputs": {
      "model": [
        "1",
        0
      ],
      "text": "{{TEXT}}",
      "seed": "{{SEED}}",
      "speed": "{{SPEED}}"
    }
  },
  "3": {
    "class_type": "SaveAudio",
    "inputs": {
      "audio": [
        "2",
        0
      ],
      "filename_prefix": "tts_output"
    }
  },
  "4": {
    "class_type": "GetAudioDuration",
    "_comment": "Output duration for video sync calculations",
    "inputs": {
      "audio": [
        "2",
        0
      ]
    }
  }
}