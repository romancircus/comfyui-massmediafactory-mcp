{
  "_meta": {
    "description": "Qwen3-TTS Custom Voice - High-quality TTS with 9 preset premium voices",
    "type": "audio_tts",
    "use_case": "Generate spoken audio with preset professional voices (replaces ElevenLabs)",
    "replaces": "ElevenLabs TTS",
    "parameters": ["TEXT", "SPEAKER", "LANGUAGE", "INSTRUCT", "SEED"],
    "defaults": {
      "SPEAKER": "Ryan",
      "LANGUAGE": "Auto",
      "INSTRUCT": "",
      "SEED": 42
    },
    "agent_notes": {
      "TEXT": "Text to synthesize (supports long content)",
      "SPEAKER": "Preset voice: Ryan, Aiden (English), Vivian, Serena, Uncle_Fu, Dylan, Eric (Chinese), Ono_Anna (Japanese), Sohee (Korean)",
      "LANGUAGE": "Auto, English, Chinese, Japanese, Korean, German, French, Russian, Portuguese, Spanish, Italian",
      "INSTRUCT": "Emotional modifier (e.g., 'Very happy', 'Calm and professional', 'Excited')",
      "quality": "0.95 voice similarity, 97ms latency, Apache 2.0 license",
      "output_formula": "For video sync: video_frames = (audio_duration_seconds * fps)"
    },
    "custom_nodes_required": ["ComfyUI-Qwen3-TTS"],
    "models_required": ["Qwen/Qwen3-TTS-12Hz-1.7B-CustomVoice"],
    "vram_required_gb": 4
  },
  "1": {
    "class_type": "Qwen3TTSLoader",
    "_comment": "Load Qwen3-TTS 1.7B CustomVoice model",
    "inputs": {
      "repo_id": "Qwen/Qwen3-TTS-12Hz-1.7B-CustomVoice",
      "source": "huggingface",
      "attention": "auto"
    }
  },
  "2": {
    "class_type": "Qwen3TTSCustomVoice",
    "_comment": "Generate speech with preset premium voice",
    "inputs": {
      "model": ["1", 0],
      "text": "{{TEXT}}",
      "speaker": "{{SPEAKER}}",
      "language": "{{LANGUAGE}}",
      "instruct": "{{INSTRUCT}}",
      "seed": "{{SEED}}"
    }
  },
  "3": {
    "class_type": "SaveAudio",
    "inputs": {
      "audio": ["2", 0],
      "filename_prefix": "qwen3_tts_output"
    }
  },
  "4": {
    "class_type": "GetAudioDuration",
    "_comment": "Output duration for video sync: frames = duration_seconds * fps",
    "inputs": {
      "audio": ["2", 0]
    }
  }
}
