{
  "_meta": {
    "description": "LTX-2 19B video extension \u2014 generate continuation frames from an existing video",
    "model": "LTX-2 19B Dev FP8",
    "type": "vid2vid",
    "parameters": [
      "PROMPT",
      "NEGATIVE",
      "SEED",
      "SOURCE_VIDEO",
      "WIDTH",
      "HEIGHT",
      "NUM_NEW_FRAMES",
      "FRAME_OVERLAP",
      "EXTEND_STRENGTH",
      "FPS",
      "STEPS",
      "PREFIX",
      "CFG_VALUES",
      "STG_SCALE_VALUES",
      "STG_RESCALE_VALUES",
      "STG_LAYERS"
    ],
    "defaults": {
      "WIDTH": 768,
      "HEIGHT": 512,
      "SEED": 42,
      "NUM_NEW_FRAMES": 80,
      "FRAME_OVERLAP": 16,
      "EXTEND_STRENGTH": 0.5,
      "FPS": 24,
      "STEPS": 30,
      "PREFIX": "ltx2_extended",
      "NEGATIVE": "worst quality, inconsistent motion, blurry, jittery, distorted",
      "CFG_VALUES": "8, 6, 6, 4, 3, 1",
      "STG_SCALE_VALUES": "4, 4, 3, 2, 1, 0",
      "STG_RESCALE_VALUES": "1, 1, 1, 1, 1, 1",
      "STG_LAYERS": "[29], [29], [29], [29], [29], [29]"
    },
    "performance": {
      "vram_gb": 20,
      "quality": "Video continuation with temporal consistency"
    },
    "vram_min": 14,
    "tags": [
      "model:ltx",
      "capability:extend",
      "capability:vid2vid",
      "priority:recommended"
    ],
    "version": "1.0.0",
    "notes": "Extends an existing video by generating continuation frames using LTXVExtendSampler. Source video is encoded via LTXVAddGuide at frame_idx=0, then new frames are generated with overlap conditioning. Requires STGGuiderAdvanced.",
    "hub_hash": "2a37c616f46f2b47"
  },
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "_meta": {
      "title": "Load LTX-2 Model"
    },
    "inputs": {
      "ckpt_name": "ltx-2-19b-dev-fp8.safetensors"
    }
  },
  "2": {
    "class_type": "LTXAVTextEncoderLoader",
    "_meta": {
      "title": "Load Gemma-3 Text Encoder"
    },
    "inputs": {
      "text_encoder": "gemma_3_12B_it.safetensors",
      "ckpt_name": "ltx-2-19b-dev-fp8.safetensors",
      "device": "default"
    }
  },
  "3": {
    "class_type": "LTXVAudioVAELoader",
    "_meta": {
      "title": "Load Audio VAE"
    },
    "inputs": {
      "ckpt_name": "ltx-2-19b-dev-fp8.safetensors"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Encode Positive Prompt"
    },
    "inputs": {
      "text": "{{PROMPT}}",
      "clip": [
        "2",
        0
      ]
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Encode Negative Prompt"
    },
    "inputs": {
      "text": "{{NEGATIVE}}",
      "clip": [
        "2",
        0
      ]
    }
  },
  "6": {
    "class_type": "LTXVConditioning",
    "_meta": {
      "title": "Apply Frame Rate Conditioning"
    },
    "inputs": {
      "positive": [
        "4",
        0
      ],
      "negative": [
        "5",
        0
      ],
      "frame_rate": "{{FPS}}"
    }
  },
  "25": {
    "class_type": "VHS_LoadVideo",
    "_meta": {
      "title": "Load Source Video"
    },
    "inputs": {
      "video": "{{SOURCE_VIDEO}}",
      "force_rate": "{{FPS}}",
      "force_size": "Disabled",
      "frame_load_cap": 0,
      "skip_first_frames": 0,
      "select_every_nth": 1
    }
  },
  "7": {
    "class_type": "EmptyLTXVLatentVideo",
    "_meta": {
      "title": "Create Latent for Total Length"
    },
    "inputs": {
      "width": "{{WIDTH}}",
      "height": "{{HEIGHT}}",
      "length": "{{NUM_NEW_FRAMES}}",
      "batch_size": 1
    }
  },
  "26": {
    "class_type": "LTXVAddGuide",
    "_meta": {
      "title": "Add Source Video as Guide"
    },
    "inputs": {
      "positive": [
        "6",
        0
      ],
      "negative": [
        "6",
        1
      ],
      "vae": [
        "1",
        2
      ],
      "latent": [
        "7",
        0
      ],
      "image": [
        "25",
        0
      ],
      "frame_idx": 0,
      "strength": 1.0
    }
  },
  "10": {
    "class_type": "LTXVScheduler",
    "_meta": {
      "title": "LTX Scheduler"
    },
    "inputs": {
      "steps": "{{STEPS}}",
      "max_shift": 2.05,
      "base_shift": 0.95,
      "stretch": true,
      "terminal": 0.1,
      "latent": [
        "26",
        2
      ]
    }
  },
  "11": {
    "class_type": "RandomNoise",
    "_meta": {
      "title": "Random Noise"
    },
    "inputs": {
      "noise_seed": "{{SEED}}"
    }
  },
  "12": {
    "class_type": "KSamplerSelect",
    "_meta": {
      "title": "Select Sampler"
    },
    "inputs": {
      "sampler_name": "euler"
    }
  },
  "13": {
    "class_type": "STGGuiderAdvanced",
    "_meta": {
      "title": "STG Guider Advanced (Required by ExtendSampler)"
    },
    "inputs": {
      "model": [
        "1",
        0
      ],
      "positive": [
        "26",
        0
      ],
      "negative": [
        "26",
        1
      ],
      "skip_steps_sigma_threshold": 0.998,
      "cfg_star_rescale": true,
      "sigmas": "1.0, 0.9933, 0.9850, 0.9767, 0.9008, 0.6180",
      "cfg_values": "{{CFG_VALUES}}",
      "stg_scale_values": "{{STG_SCALE_VALUES}}",
      "stg_rescale_values": "{{STG_RESCALE_VALUES}}",
      "stg_layers_indices": "{{STG_LAYERS}}"
    }
  },
  "27": {
    "class_type": "LTXVExtendSampler",
    "_meta": {
      "title": "Extend Sampler"
    },
    "inputs": {
      "model": [
        "1",
        0
      ],
      "vae": [
        "1",
        2
      ],
      "latents": [
        "26",
        2
      ],
      "num_new_frames": "{{NUM_NEW_FRAMES}}",
      "frame_overlap": "{{FRAME_OVERLAP}}",
      "guider": [
        "13",
        0
      ],
      "sampler": [
        "12",
        0
      ],
      "sigmas": [
        "10",
        0
      ],
      "noise": [
        "11",
        0
      ],
      "strength": "{{EXTEND_STRENGTH}}"
    }
  },
  "16": {
    "class_type": "VAEDecodeTiled",
    "_meta": {
      "title": "Decode Extended Video"
    },
    "inputs": {
      "samples": [
        "27",
        0
      ],
      "vae": [
        "1",
        2
      ],
      "tile_size": 512,
      "overlap": 64,
      "temporal_size": 64,
      "temporal_overlap": 8
    }
  },
  "18": {
    "class_type": "CreateVideo",
    "_meta": {
      "title": "Create Video"
    },
    "inputs": {
      "images": [
        "16",
        0
      ],
      "fps": "{{FPS}}"
    }
  },
  "19": {
    "class_type": "SaveVideo",
    "_meta": {
      "title": "Save Video"
    },
    "inputs": {
      "video": [
        "18",
        0
      ],
      "filename_prefix": "{{PREFIX}}",
      "format": "mp4",
      "codec": "auto"
    }
  }
}
